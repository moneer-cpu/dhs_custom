<odoo>
    <data>

        <!-- تعريف الصفحة الجديدة -->
        <template id="blog_standard_page" name="Blog Standard Page">
            <t t-call="website.layout">
                <div id="wrap">
                    <!-- محتوى الصفحة الذي أرسلته -->
                    <div class="breadcumb-wrapper" data-bg-src="/dhs/static/src/img/bg/breadcumb-bg.jpg">
                        <div class="container">
                            <div class="breadcumb-content">
                                <h1 class="breadcumb-title">Blog Standard</h1>
                                <ul class="breadcumb-menu">
                                    <li>
                                        <a t-attf-href="/">Home</a>
                                    </li>
                                    <li>Blog Standard</li>
                                </ul>
                            </div>
                        </div>
                    </div>

                    <!-- ==============================
                    Blog Section
                    ============================== -->
                    <section class="th-blog-wrapper space-top space-extra-bottom">
                        <div class="container">
                            <div class="row">
                                <!-- Blog Content -->
                                <div class="col-xxl-8 col-lg-7">

                                    <!-- Blog Post 1 -->
                                    <div class="th-blog blog-single has-post-thumbnail">
                                        <div class="blog-img">
                                            <a href="#">
                                                <img src="/dhs/static/src/img/blog/blog-s-1-1.jpg"
                                                     alt="Blog Image"/>
                                            </a>
                                        </div>
                                        <div class="blog-content">
                                            <div class="blog-meta">
                                                <a class="author" href="#"><i class="fal fa-user"></i>By Mediax
                                                </a>
                                                <a href="#"><i class="fal fa-calendar"></i>21 June, 2024
                                                </a>
                                                <a href="#"><i class="fal fa-comments"></i>Comments (3)
                                                </a>&amp;
                                            </div>
                                            <h2 class="blog-title">
                                                <a href="#">How Business Is Taking Over  What to Do About It</a>
                                            </h2>
                                            <p>We offer flexible appointment scheduling options to accommodate your busy
                                                lifestyle...
                                            </p>
                                            <a href="#" class="th-btn btn-sm">Read More</a>
                                        </div>
                                    </div>

                                    <!-- Blog Post 2 (Slider) -->
                                    <div class="th-blog blog-single has-post-thumbnail">
                                        <div class="blog-img th-slider" data-slider-options='{"effect":"fade"}'>
                                            <div class="swiper-wrapper">
                                                <div class="swiper-slide">
                                                    <a href="#">
                                                        <img src="/dhs/static/src/img/blog/blog-s-1-2.jpg"
                                                             alt="Blog Image"/>
                                                    </a>
                                                </div>
                                                <div class="swiper-slide">
                                                    <a href="#">
                                                        <img src="/dhs/static/src/img/blog/blog-s-1-4.jpg"
                                                             alt="Blog Image"/>
                                                    </a>
                                                </div>
                                            </div>
                                            <button class="slider-arrow slider-prev">
                                                <i class="far fa-arrow-left"></i>
                                            </button>
                                            <button class="slider-arrow slider-next">
                                                <i class="far fa-arrow-right"></i>
                                            </button>
                                        </div>
                                        <div class="blog-content">
                                            <div class="blog-meta">
                                                <a class="author" href="#"><i class="fal fa-user"></i>By Mediax
                                                </a>
                                                <a href="#"><i class="fal fa-calendar"></i>22 June, 2024
                                                </a>
                                                <a href="#"><i class="fal fa-comments"></i>Comments (3)
                                                </a>
                                            </div>
                                            <h2 class="blog-title">
                                                <a href="#">Health vs. Wealth Navigate Business in Medicine</a>
                                            </h2>
                                            <p>We offer flexible appointment scheduling options to accommodate your busy
                                                lifestyle...
                                            </p>
                                            <a href="#" class="th-btn btn-sm">Read More</a>
                                        </div>
                                    </div>

                                    <!-- Blog Post 3 -->
                                    <div class="th-blog blog-single">
                                        <div class="blog-content">
                                            <div class="blog-meta">
                                                <a class="author" href="#"><i class="fal fa-user"></i>By Mediax
                                                </a>
                                                <a href="#"><i class="fal fa-calendar"></i>23 June, 2024
                                                </a>
                                                <a href="#"><i class="fal fa-comments"></i>Comments (3)
                                                </a>
                                            </div>
                                            <h2 class="blog-title">
                                                <a href="#">Preserving Care Strategy Amidst Food Changes</a>
                                            </h2>
                                            <p>We offer flexible appointment scheduling options to accommodate your busy
                                                lifestyle...
                                            </p>
                                            <a href="#" class="th-btn btn-sm">Read More</a>
                                        </div>
                                    </div>

                                    <!-- Blog Post 4 (Video) -->
                                    <div class="th-blog blog-single has-post-thumbnail">
                                        <div class="blog-img" data-overlay="title" data-opacity="4">
                                            <a href="#">
                                                <img src="/dhs/static/src/img/blog/blog-s-1-3.jpg"
                                                     alt="Blog Image"/>
                                            </a>
                                            <a href="https://www.youtube.com/watch?v=_sI_Ps7JSEk"
                                               class="play-btn popup-video">
                                                <i class="fas fa-play"></i>
                                            </a>
                                        </div>
                                        <div class="blog-content">
                                            <div class="blog-meta">
                                                <a class="author" href="#"><i class="fal fa-user"></i>By Mediax
                                                </a>
                                                <a href="#"><i class="fal fa-calendar"></i>24 June, 2024
                                                </a>
                                                <a href="#"><i class="fal fa-comments"></i>Comments (3)
                                                </a>
                                            </div>
                                            <h2 class="blog-title">
                                                <a href="#">Healthcare Tips Navigating Business &amp; Patient Care</a>
                                            </h2>
                                            <p>We offer flexible appointment scheduling options to accommodate your busy
                                                lifestyle...
                                            </p>
                                            <a href="#" class="th-btn btn-sm">Read More</a>
                                        </div>
                                    </div>

                                    <!-- Blog Post 5 (Audio) -->
                                    <div class="th-blog blog-single has-post-thumbnail">
                                        <div class="blog-audio">
                                            <iframe title="Tell Me U Luv Me (with Trippie Redd) by Juice WRLD"
                                                    src="https://w.soundcloud.com/player/?visual=true&amp;url=https%3A%2F%2Fapi.soundcloud.com%2Ftracks%2F830279092&amp;show_artwork=true&amp;maxwidth=751&amp;maxheight=1000&amp;dnt=1"></iframe>
                                        </div>
                                        <div class="blog-content">
                                            <div class="blog-meta">
                                                <a class="author" href="#"><i class="fal fa-user"></i>By Mediax
                                                </a>
                                                <a href="#"><i class="fal fa-calendar"></i>27 June, 2024
                                                </a>
                                                <a href="#"><i class="fal fa-comments"></i>Comments (3)
                                                </a>
                                            </div>
                                            <h2 class="blog-title">
                                                <a href="#">Strategies Against Corporate Intrusion in Healthcare</a>
                                            </h2>
                                            <p>We offer flexible appointment scheduling options to accommodate your busy
                                                lifestyle...
                                            </p>
                                            <a href="#" class="th-btn btn-sm">Read More</a>
                                        </div>
                                    </div>

                                    <!-- Pagination -->
                                    <div class="th-pagination text-center">
                                        <ul>
                                            <li>
                                                <a href="#">1</a>
                                            </li>
                                            <li>
                                                <a href="#">2</a>
                                            </li>
                                            <li>
                                                <a href="#">3</a>
                                            </li>
                                            <li>
                                                <a href="#">
                                                    <i class="far fa-arrow-right"></i>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>

                                </div>

                                <!-- Sidebar -->
                                <div class="col-xxl-4 col-lg-5">
                                    <aside class="sidebar-area">
                                        <div class="widget widget_search">
                                            <form class="search-form">
                                                <input type="text" placeholder="Enter Keyword"/>
                                                <button type="submit">
                                                    <i class="far fa-search"></i>
                                                </button>
                                            </form>
                                        </div>

                                        <div class="widget widget_categories">
                                            <h3 class="widget_title">Categories</h3>
                                            <ul>
                                                <li>
                                                    <a href="#">Cardiology</a>
                                                </li>
                                                <li>
                                                    <a href="#">Gastroenterologist</a>
                                                </li>
                                                <li>
                                                    <a href="#">Dental Care</a>
                                                </li>
                                                <li>
                                                    <a href="#">Ophthalmology</a>
                                                </li>
                                                <li>
                                                    <a href="#">Orthopedics</a>
                                                </li>
                                                <li>
                                                    <a href="#">Plastic Surgeons</a>
                                                </li>
                                                <li>
                                                    <a href="#">Neurology</a>
                                                </li>
                                            </ul>
                                        </div>

                                        <div class="widget">
                                            <h3 class="widget_title">Recent Posts</h3>
                                            <div class="recent-post-wrap">
                                                <t t-foreach="[1,2,3]" t-as="post">
                                                    <div class="recent-post">
                                                        <div class="media-img">
                                                            <a href="#">
                                                                <img t-att-src="'/dhs/static/src/img/blog/recent-post-1-' + str(post) + '.jpg'"
                                                                     alt="Blog Image"/>
                                                            </a>
                                                        </div>
                                                        <div class="media-body">
                                                            <h4 class="post-title">
                                                                <a href="#" class="text-inherit">Sample Post Title</a>
                                                            </h4>
                                                            <div class="recent-post-meta">
                                                                <a href="#"><i class="fal fa-calendar"></i>21 Jun, 2024
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </t>
                                            </div>
                                        </div>

                                        <div class="widget widget_tag_cloud">
                                            <h3 class="widget_title">Popular Tags</h3>
                                            <div class="tagcloud">
                                                <a href="#">Skin Care</a>
                                                <a href="#">Advice</a>
                                                <a href="#">Solution</a>
                                                <a href="#">Doctors</a>
                                                <a href="#">Medical</a>
                                                <a href="#">Hospital</a>
                                            </div>
                                        </div>
                                    </aside>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </t>
        </template>

        <!-- تعريف المسار للرابط -->
        <record id="route_blog_standard" model="website.page">
            <field name="name">Blog Standard</field>
            <field name="url">/blog-standard</field>
            <field name="view_id" ref="dhs.blog_standard_page"/>
<!--            <field name="website_published">True</field>-->
        </record>

    </data>
</odoo>
